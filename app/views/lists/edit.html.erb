<h1>Edit List</h1>
<%= form_for @list do |f| %>
<%= f.text_field :title, placeholder: "Title" %>
<%= f.submit class: 'btn btn-success'%>
<% end %>

<div class="btn-group" role="group" aria-label="Basic example">
    <%= button_to 'Cancel', list_path, method: 'get', class: 'btn btn-secondary'%>
    <%= button_to 'Delete', list_path, method: 'delete', class: 'btn btn-danger' %>
</div>